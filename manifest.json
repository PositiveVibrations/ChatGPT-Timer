{
    "manifest_version": 3,
    "name": "ChatGPT Timer",
    "version": "1.0",
    "description": "Set a timer to remind you when to use ChatGPT4.",
    "permissions": ["alarms", "notifications"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  